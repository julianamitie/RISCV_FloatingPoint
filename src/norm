    %store/vec4 v0x5571563e7760_0, 0, 23;
    %load/vec4 v0x5571563d4620_0;
    %addi 1, 0, 8;
    %store/vec4 v0x5571563e7590_0, 0, 8;
T_0.1 ;
    %jmp T_0;
